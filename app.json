{
  "expo": {
    "name": "Study Buddy",
    "slug": "study-buddy-4fpqfs7",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.study-buddy-4fpqfs7",
      "infoPlist": {
        "NSCameraUsageDescription": "Study Buddy uses the camera to scan handwritten notes and convert them to digital text. For example, you can take a photo of your lecture notes and the app will extract the text to create searchable digital notes and flashcards.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.study-buddy-4fpqfs7",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Study Buddy uses the camera to scan handwritten notes and convert them to digital text. For example, you can take a photo of your lecture notes and the app will extract the text to create searchable digital notes and flashcards.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}